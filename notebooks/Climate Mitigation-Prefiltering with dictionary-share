                                         ...  \n","33292                                                0  \n","33293                                                0  \n","33294                                                0  \n","33295                                                0  \n","33296                                                0  \n","\n","[33297 rows x 15 columns]>\n"]},{"output_type":"stream","name":"stderr","text":["\n"]}],"source":["\n","\n","# Load SpaCy model\n","nlp = spacy.load(\"en_core_web_sm\")\n","\n","# Your high-precision climate mitigation dictionary (lemmatized)\n","CLIMATE_DICTIONARY_RAW = [\n","    \"carbon neutrality\",\n","    \"climate neutrality\",\n","    \"net zero\",\n","    \"zero emission\",\n","    \"greenhouse gas emission\",\n","    \"ghg emission\",\n","    \"carbon emission\",\n","    \"emission trading system\",\n","    \"ets reform\",\n","    \"carbon border adjustment\",\n","    \"cbam\",\n","    \"carbon pricing\",\n","    \"carbon footprint\",\n","    \"carbon tax\",\n","    \"co2 reduction\",\n","    \"climate mitigation\",\n","    \"energy transition\",\n","    \"renewable energy deployment\",\n","    \"decarbonisation\",\n","    \"eu climate target\",\n","    \"paris agreement\",\n","    \"climate law\",\n","    \"fit for 55\",\n","    \"green deal\",\n","    \"climate neutrality goal\",\n","    \"energy efficiency directive\",\n","    \"emission reduction target\"\n","]\n","def lemmatize_phrase(phrase):\n","    doc = nlp(phrase)\n","    return ' '.join([token.lemma_.lower() for token in doc if not token.is_punct and not token.is_space])\n","\n","# Preprocess dictionary\n","CLIMATE_DICTIONARY = [lemmatize_phrase(p) for p in CLIMATE_DICTIONARY_RAW]\n","\n","# Lemmatize and normalize a sentence\n","def lemmatize_text(text):\n","    doc = nlp(text)\n","    return ' '.join(token.lemma_.lower() for token in doc if not token.is_punct and not token.is_space)\n","\n","# Count how many dictionary phrases match\n","def count_dict_matches(sentence, dictionary):\n","    lemmatized = lemmatize_text(sentence)\n","    tokens = lemmatized.split()\n","    match_count = 0\n","\n","    for phrase in dictionary:\n","        phrase_tokens = phrase.split()\n","        n = len(phrase_tokens)\n","\n","        for i in range(len(tokens) - n + 1):\n","            if tokens[i:i + n] == phrase_tokens:\n","                match_count += 1\n","                break  # Count only once per phrase\n","    return match_count\n","'''\n","# --- Example with DataFrame ---\n","# Replace this with your actual DataFrame\n","df = pd.DataFrame({\n","    \"sentence\": [\n","        \"The EU aims for climate neutrality by 2050 through the Green Deal.\",\n","        \"This policy improves agricultural subsidies and food safety.\",\n","        \"We are reducing CO2 emissions and enhancing the ETS.\",\n","        \"Nothing related to climate here.\"\n","    ]\n","})\n","'''\n","tqdm.pandas()\n","\n","df['how_many_words_in_climate_mitigation_dictionary'] = df['sentence'].progress_apply(\n","    lambda sent: count_dict_matches(sent, CLIMATE_DICTIONARY)\n",")\n","# View result\n","print(df.head)\n"]},{"cell_type":"code","source":["# Ensure pyarrow or feather-format is installed\n","#!pip install -q pyarrow\n","import os\n","\n","output_dir = \"/content/drive/My Drive/Climate Mitigation Council Sentences/\"\n","os.makedirs(output_dir, exist_ok=True)  # Creates the directory if it doesn't exist\n","# Save the DataFrame\n","df.to_feather(\"/content/drive/My Drive/Climate Mitigation Council Sentences/Council_sentences_climdict_filtered.feather\")"],"metadata":{"id":"R5kvIxxsnnxp"},"execution_count":null,"outputs":[]},{"cell_type":"code","source":["#df=pd.read_feather(\"/content/drive/MyDrive/Climate Mitigation Council Sentences/Council_sentences_climdict_filtered.feather\")\n","#df.to_csv(\"/content/drive/My Drive/Climate Mitigation Council Sentences/Council_sentences_climdict_filtered_.csv\", index=False)"],"metadata":{"